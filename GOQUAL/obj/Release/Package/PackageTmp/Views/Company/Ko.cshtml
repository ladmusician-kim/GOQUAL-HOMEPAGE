@model GOQUAL.Views.Company.IndexViewModel
@{
    Layout = "~/Views/Shared/_ContentLayout.cshtml";
}
@section Style {
    <link href="~/Lib/GOQUAL/css/jquery.fullPage.css" rel="stylesheet" />
}
@section Script {
    <script src="~/Lib/GOQUAL/js/jquery.slimscroll.min.js"></script>
    <script src="~/Lib/GOQUAL/js/jquery.fullPage.js"></script>
    <script src="~/Lib/GOQUAL/js/Company/Index.js"></script>
}

@Html.HiddenFor(x => x.Lang)
<div ng-controller="CompanyCtrl">
    <header class="navbar-fixed-top">
        <!-- header -->
        <div id="mainHeader" role="banner">
            <div class="container">
                <nav class="navbar navbar-default scrollMenu" role="navigation">
                    <div class="navbar-header">
                        <!-- responsive navigation -->
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <div class="navbar-toggle">
                            <a href="/Home/Korea" class="goqual_lang_ko" style="font-size: 13px;">한국어</a>
                            |
                        <a href="/Home/English" class="goqual_lang_eng" style="font-size: 13px;">ENGLISH</a>
                        </div>
                        <!-- Logo -->
                        <a class="navbar-brand" href="/Home/Index?lang=@(Model.Lang)">
                            <img class="goqual_menu_header" src="~/Lib/GOQUAL/images/ci/1.png" alt="GOQUAL 고퀄 주식회사" />
                        </a>
                    </div>
                    <div class="collapse navbar-collapse" id="mainMenu">
                        <ul class="language_menu_dropdown nav navbar-nav pull-right goqual_lang_menu">
                            <li class="primary goqual_lang_ko">
                                <a ng-click="changeLang_ko()" class="firstLevel">한국어</a>
                            </li>
                            <li class="sep"></li>
                            <li class="primary goqual_lang_eng">
                                <a ng-click="changeLang_eng()" class="firstLevel">ENGLISH</a>
                            </li>
                        </ul>
                        <!-- Main navigation -->
                        <ul class="nav navbar-nav pull-right">
                            <li class="primary">
                                <a href="/Home/Index?lang=@(Model.Lang)" class="firstLevel">HOME</a>
                            </li>
                            <li class="sep"></li>
                            <li class="primary">
                                <a href="/Company/Index?lang=@(Model.Lang)" class="firstLevel">COMPANY</a>
                            </li>
                            <li class="sep"></li>
                            <li class="primary">
                                <a href="/Product/Index?lang=@(Model.Lang)" class="firstLevel">PRODUCT</a>
                            </li>
                            <li class="sep"></li>
                            <li class="primary">
                                <a href="/Contact/Index?lang=@(Model.Lang)" class="firstLevel">CONTACT</a>
                            </li>
                            <li class="sep"></li>
                            <li class="primary">
                                <a href="/Joinus/Index?lang=@(Model.Lang)" class="firstLevel">JOIN US</a>
                            </li>
                            <li class="sep"></li>
                            <li class="primary">
                                <a href="/Info/Index?lang=@(Model.Lang)" class="firstLevel">NEWS</a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
    </header>

    <section class="tabsDetails goqual_container" data-nekoawnim="fadeInUp" data-nekodelay="100">
        <div id="fullpage">
            <div class="section ">
                <iframe width="100%" height="100%" 
                    src="//www.youtube.com/embed/9hZJgRE_Nhw" frameborder="0" allowfullscreen></iframe>
            </div>
            <div class="section align_center">
                <div class="img_container">
                    <img src="~/Lib/GOQUAL/images/company/ko/company_1.png" />
                </div>
            </div>
            <div class="section align_center">
                <div class="img_container">
                    <img src="~/Lib/GOQUAL/images/company/ko/company_2.png" />
                </div>
            </div>
            <div class="section align_center">
                <div class="img_container">
                    <img src="~/Lib/GOQUAL/images/company/ko/company_3.png" style="margin-top: 3%;"/>
                    <section id="team" class="pt40 pb40">

                        <div class="container">
                            <div class="row">
                                <section class="col-md-12">
                                    <div class="row mb15">
                                        <div class="col-sm-3 nekoAnim-invisible nekoAnim-animated nekoAnim-fadeIn nekoAnim-visible" data-nekoanim="fadeIn" data-nekodelay="100" style="-webkit-animation: 100ms;">
                                            <article>
                                                <div>
                                                    <img src="~/Lib/GOQUAL/images/member/member_1.png" class="img-responsive goqual_member"/>
                                                </div>
                                                <div class="boxContent text-center">
                                                    <h3>우상범</h3>
                                                    <p style="bottom: -50px;">CEO</p>
                                                </div>
                                            </article>
                                        </div>
                                        <div class="col-sm-3 nekoAnim-invisible nekoAnim-animated nekoAnim-fadeIn nekoAnim-visible" data-nekoanim="fadeIn" data-nekodelay="300" style="-webkit-animation: 300ms;">
                                            <article>
                                                <div>
                                                    <img src="~/Lib/GOQUAL/images/member/member_2.png" class="img-responsive goqual_member"/>
                                                </div>
                                                <div class="boxContent text-center">
                                                    <h3>최병일</h3>
                                                    <p style="bottom: -50px;">R&D</p>
                                                </div>
                                            </article>
                                        </div>
                                        <div class="col-sm-3 nekoAnim-invisible nekoAnim-animated nekoAnim-fadeIn nekoAnim-visible" data-nekoanim="fadeIn" data-nekodelay="400" style="-webkit-animation: 400ms;">
                                            <article>
                                                <div>
                                                    <img src="~/Lib/GOQUAL/images/member/seungjun.PNG" class="img-responsive goqual_member"/>
                                                </div>
                                                <div class="boxContent text-center">
                                                    <h3>이승준</h3>
                                                    <p style="bottom: -50px;">R&D</p>
                                                </div>
                                            </article>
                                        </div>
                                        <div class="col-sm-3 nekoAnim-invisible nekoAnim-animated nekoAnim-fadeIn nekoAnim-visible" data-nekoanim="fadeIn" data-nekodelay="200" style="-webkit-animation: 200ms;">
                                            <article>
                                                <div>
                                                    <img src="~/Lib/GOQUAL/images/member/dongjin.PNG" class="img-responsive goqual_member"/>
                                                </div>
                                                <div class="boxContent text-center">
                                                    <h3>김동진</h3>
                                                    <p style="bottom: -50px;">R&D</p>
                                                </div>
                                            </article>
                                        </div>
                                    </div>
                                </section>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
            <div class="section align_center">
                <div class="img_container">
                    <img src="~/Lib/GOQUAL/images/company/ko/company_4.png" />
                </div>
            </div>
            <div class="section align_center">
                <div class="img_container">
                    <img src="~/Lib/GOQUAL/images/company/ko/company_5.png" style="width: 58%" />
                </div>
            </div>
        </div>
    </section>
</div>
